<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  
    <system.serviceModel>
      <services>
        <service name="BankService.DataServiceManager">
          <endpoint address="net.tcp://localhost:8010/BankDataService"
                    binding="netTcpBinding" 
                    bindingConfiguration="tcpBinding1"
                    contract="Service.Common.IDataService" />
          <endpoint address="net.tcp://localhost:8010/BankDataService"
                  binding="netTcpBinding"
                  bindingConfiguration="tcpBinding1"
                  contract="Service.Common.IPingService" />
        </service>
        <service name="BankService.ReportServiceManager">
          <endpoint address="net.tcp://localhost:8011/BankReportService" 
                    binding="netTcpBinding" 
                    bindingConfiguration="tcpBinding2"
                    contract="Service.Common.IReportService" />
          <endpoint address="net.tcp://localhost:8011/BankReportService"
                   binding="netTcpBinding"
                   bindingConfiguration="tcpBinding2"
                   contract="Service.Common.IPingService" />
        </service>
      </services>
      <bindings>
        <netTcpBinding>
          <binding name="tcpBinding1" transactionFlow="true" />
          <binding name="tcpBinding2" />
        </netTcpBinding>
      </bindings>
    </system.serviceModel>
</configuration>